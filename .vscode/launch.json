  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "OpenAPI server",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/node_modules/daf-cli/build/cli.js",
      "args": ["server"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "well known",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/node_modules/daf-cli/build/cli.js",
      "args": ["execute", "-m", "verifyWellKnownDidConfiguration", "-a", "{\"domain\": \"identity.foundation\"}"],
      "sourceMaps": true,
      //"resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"]
    }
  ]
}
