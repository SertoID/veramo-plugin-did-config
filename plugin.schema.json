{
  "IWellKnownDidConfigurationPlugin": {
    "components": {
      "schemas": {
        "IWellKnownDidConfigurationPluginArgs": {
          "type": "object",
          "properties": {
            "did": {
              "type": "string",
              "description": "Decentralized identifier"
            },
            "dids": {
              "type": "array",
              "items": {
                "type": "string"
              },
              "description": "List of DIDs to be included in the DID configuration file"
            },
            "domain": {
              "type": "string",
              "description": "The domain name that controlls the DIDs"
            }
          },
          "required": [
            "did",
            "dids",
            "domain"
          ],
          "description": "Arguments needed for myPluginFoo"
        }
      },
      "methods": {
        "generateDidConfiguration": {
          "description": "Generates a DID configuration for a domain including a list of DID \"owned\" by the DID agent.",
          "arguments": {
            "$ref": "#/components/schemas/IWellKnownDidConfigurationPluginArgs"
          },
          "returnType": {
            "type": "string"
          }
        }
      }
    }
  }
}